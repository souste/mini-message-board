<html>
  <head>
    <title>Mini Messageboard</title>
  </head>
  <body>
    <h1>Mini Messageboard</h1>
    <a href="/new">New Message</a>
    <ul>
      <% for (let i = 0; i < messages.length; i++) { %>
      <li>
        <h3>USER: <%= messages[i].user %></h3>
        <p>Message: <%= messages[i].text %></p>
        <p>Date: <%= messages[i].added %></p>
      </li>
      <% } %>
    </ul>
  </body>
</html>
